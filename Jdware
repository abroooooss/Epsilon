local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Epsilon", HidePremium = true, SaveConfig = true, ConfigFolder = "OrionTest"})
local plr = game.Players.LocalPlayer

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Tab2 = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



Tab2:AddButton({
	Name = "No kill bricks",
	Callback = function()
      		for i,v in pairs(workspace.Map.KillBricks:GetDescendants()) do
		if v:IsA("TouchTransmitter") or v:IsA("Script") then
			v:Destroy()
		end
	end
  	end    
})

Tab2:AddButton({
	Name = "No Fall Damage",
	Callback = function()
      		plr.Character.FallDamage.Disabled = true
  	end    
})

Tab:AddButton({
	Name = "Auto Trinket Pickup",
	Callback = function()
      		 while true do
       for _, trinket in pairs (game.Workspace.MouseIgnore:GetDescendants()) do
           if trinket:IsA("ClickDetector") then
               fireclickdetector(trinket)
           end    
       end
       wait(0.3)
end
end
})

Tab:AddButton({
    Name = "Dupe Item",
    Callback = function()
        
        plr.Character.Humanoid:Destroy()
        wait(1)
        game:GetService("Players").LocalPlayer.OnTeleport:Connect(function(State)
            if State == Enum.TeleportState.Started then
                syn.queue_on_teleport([[
                    repeat wait()
                    until game:IsLoaded() 
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/abroooooss/Epsilon/main/Jdware"))()
                ]])
            end
        end)
	wait(3)
        game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
    end    
})
Tab2:AddButton({
	Name = "Esp",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua'))()
  	end    
})

Tab2:AddButton({
	Name = "FullBright",
	Callback = function()
      		loadstring(game:HttpGet("https://pastebin.com/raw/06iG6YkU", true))()
  	end    
})


OrionLib:Init()
